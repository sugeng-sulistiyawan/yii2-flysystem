{
	"name": "diecoding/yii2-flysystem",
	"description": "The League Flysystem for local anda remote filesystems library for Yii2",
	"authors": [
		{
			"name": "Sugeng Sulistiyawan",
			"email": "sugeng.sulistiyawan@gmail.com"
		}
	],
	"type": "library",
	"keywords": [
		"yii2",
		"extension",
		"component",
		"flysystem",
		"aws",
		"s3",
        "files",
        "azure",
        "dropbox",
        "ftp",
        "gridfs",
        "rackspace",
        "sftp",
        "webdav"
	],
	"license": "MIT",
	"support": {
		"issues": "https://github.com/sugeng-sulistiyawan/yii2-flysystem/issues",
		"source": "https://github.com/sugeng-sulistiyawan/yii2-flysystem"
	},
	"require": {
		"php": ">=8.0",
		"league/flysystem": "^3.14",
		"yiisoft/yii2": "~2.0"
	},
	"require-dev": {
		"justus/flysystem-onedrive": "^2.0",
		"league/flysystem-async-aws-s3": "^3.13",
		"league/flysystem-aws-s3-v3": "^3.13",
		"league/flysystem-azure-blob-storage": "^3.13",
		"league/flysystem-ftp": "^3.13",
		"league/flysystem-google-cloud-storage": "^3.13",
		"league/flysystem-memory": "^3.13",
		"league/flysystem-path-prefixing": "^3.13",
		"league/flysystem-read-only": "^3.13",
		"league/flysystem-sftp-v3": "^3.14",
		"league/flysystem-webdav": "^3.13",
		"masbug/flysystem-google-drive-ext": "^2.2",
		"phpunit/phpunit": "~9.5.0",
		"platformcommunity/flysystem-bunnycdn": "^3.3",
		"royvoetman/flysystem-gitlab-storage": "^3.0",
		"shitware-ltd/flysystem-msgraph": "^1.0",
		"spatie/flysystem-dropbox": "^3.0"
	},
	"autoload": {
		"psr-4": {
			"diecoding\\flysystem\\": "src/"
		}
	},
    "autoload-dev": {
        "psr-4": {
            "diecoding\\flysystem\\tests\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        },
        "sort-packages": true
    }
}
