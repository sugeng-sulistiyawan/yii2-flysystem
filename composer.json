{
	"name": "diecoding/yii2-flysystem",
	"description": "The League Flysystem for local and remote filesystems library for Yii2",
	"authors": [
		{
			"name": "Sugeng Sulistiyawan",
			"email": "sugeng.sulistiyawan@gmail.com"
		}
	],
	"type": "library",
	"keywords": [
		"yii2",
		"extension",
		"component",
		"flysystem",
		"filesystem",
        "files",
		"aws",
		"s3",
        "ftp"
	],
	"license": "MIT",
	"support": {
		"issues": "https://github.com/sugeng-sulistiyawan/yii2-flysystem/issues",
		"source": "https://github.com/sugeng-sulistiyawan/yii2-flysystem"
	},
	"require": {
		"php": ">=8.0",
		"league/flysystem": "^3.14",
		"yiisoft/yii2": "~2.0"
	},
	"require-dev": {
		"justus/flysystem-onedrive": "^2.0",
		"league/flysystem-aws-s3-v3": "^3.15",
		"league/flysystem-ftp": "^3.15"
	},
	"autoload": {
		"psr-4": {
			"diecoding\\flysystem\\": "src/"
		}
	},
    "autoload-dev": {
        "psr-4": {
            "diecoding\\flysystem\\tests\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        },
        "sort-packages": true
    }
}
